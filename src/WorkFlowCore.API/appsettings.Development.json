{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FileStorage": {
    "DefaultProvider": "Local",
    "TempDirectory": "App_Data/dev-chunks",
    "Chunk": {
      "MinChunkSizeInBytes": 262144,
      "MaxChunkSizeInBytes": 8388608,
      "ExpirationMinutes": 60
    },
    "Local": {
      "RootPath": "App_Data/dev-files",
      "UseDateFolders": true
    },
    "Aliyun": {
      "Endpoint": "oss-cn-hangzhou.aliyuncs.com",
      "AccessKeyId": "your-aliyun-key-id",
      "AccessKeySecret": "your-aliyun-secret",
      "BucketName": "workflow-dev"
    },
    "Aws": {
      "Region": "ap-southeast-1",
      "AccessKeyId": "your-aws-key-id",
      "AccessKeySecret": "your-aws-secret",
      "BucketName": "workflow-dev"
    }
  }
}
